          <argument>
            <kind>1</kind>
            <string>/// Shop Drag Drop and clicking

var routes = data_get("routes");

if (mouse_check_button_pressed(mb_left)) {
    var route = ds_stack_top(routes);
    var item = scr_shop_get_hovered_item(map_get(route, "items"));
    var type = map_get(route, "type");
    
    if (is_array(item)) {
        var additional_data = map_get(item, "additional_data");
        
        if (type == shop_type.category) {
            ds_stack_push(routes, scr_shop_route(
                shop_type.component,
                scr_components_to_items(data_get("components"))
            ));
        }
        else if (type == shop_type.component) {
            if (scr_can_buy_item(item)) {
                scr_drag_component(additional_data);
            }
        }
    }
}
</string>
