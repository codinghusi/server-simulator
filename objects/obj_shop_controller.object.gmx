            <kind>1</kind>
            <string>///Daily Costs
alarm[0] = DAY_DURATION * room_speed


var message = "";
var total = 0;
var amount = 0;

var pname = "";

with(obj_component){
    total += data_get("power_consumtion") * global.power_multiplicator
    
    if (pname != data_get("name") &amp;&amp; pname != ""){
        if (amount != 1){
            message += "*" + string(amount)
        }
        amount = 0
    }
    if (pname != data_get("name"))
        message += "#" + string(data_get("name")) + ": " + string(data_get("power_consumtion") * global.power_multiplicator) + "$"
    amount++    
    pname = data_get("name")
}

message += "#----------#Total: " + string(total)

global.money -= total;
if (global.money &lt; 0){
    scr_gameover();
}

with(obj_controller) scr_news_post("Daily Costs", message)
</string>
