          <argument>
            <kind>1</kind>
            <string>/// Send Packet

var id_ = data_get("last_id");
var ddos = data_get("ddos");
var critical = data_get("critical");

data_increase("last_id", 1);
var timeout = irandom_range(1000 * 40, 1000 * 60);

var ddos_packet = false;
if (ddos) {
    ddos_packet = random(1) &lt; 0.90;
}

var packet_ = scr_packet(
    false,
    irandom(1),
    false,
    current_time,
    timeout,
    id_,
    ddos_packet
);

if (!ddos_packet) {
    ds_list_add(packets, packet_);
}

var probability = critical * max(0.1, global.user_reputation * 10) * clamp(current_time / 1000 / 20, 1, 10);

alarm[0] = 1 + min(random_range(10 / probability, 20 / probability), 3) * room_speed;

if (!ddos) {
    alarm[0] = max((1 - global.user_reputation) * room_speed * 2, alarm[0]);
}

scr_create_packet(
    packet_,
    downlinks[| 0]
);
</string>
