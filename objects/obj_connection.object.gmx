            <string>/// Draw drag drop feedback

var xx = scr_connection_x(self, 0.5);
var yy = scr_connection_y(self, 0.5);

if (!instance_exists(data_get("dropping_instance"))) {
    data_set("drop_possible", false);
    data_set("dropable", false);
