            <string>/// Inbox

var inbox = method_params;
var uplink = data_get("uplink");
var outgoing = method_param("outgoing");
var available = data_get("available");
var useless_check = data_get("useless_check");
var useless = method_param("useless");
var process_time = 1;

if (outgoing) {
    if (data_get("uplink")) {
        scr_create_packet(inbox, uplink);
    } else {
        // We are in obj_component_internet
        // TODO: move into obj_component_internet and handle it there
        method(self, component_method.inbox, map(
            kv("inbox", inbox)
        ));
    }
    exit;
}


if (!available) {
    scr_burn_packet(x, y, inbox);
    exit;
}

if (useless_check &amp;&amp; useless) {
    scr_component_process(self, scr_thread(inbox, process_time));
    exit;
}

method(self, component_method.inbox, map(
    kv("inbox", inbox)
));
</string>
