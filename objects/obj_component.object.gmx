            <string>/// Process threads

var threads = data_get("threads");

var length = array_length_1d(threads);
for(var i = 0; i &lt; length; i++){
    var thread = threads[i]
    if (is_array(thread)) {
        map_decrease(thread, kv("remaining_time", 1 / data_get("process_time")));
        if (map_get(thread, "remaining_time") &lt;= 0) {
            data_decrease("slots_used", 1);
            var packet = map_get(thread, "packet");
            threads[i] = noone;
            
            if (data_get("useless_check") &amp;&amp; map_get(packet, "useless")) {
                scr_burn_packet(x, y, packet);
            } else {
                method(self, component_method.thread_finish, map(
                    kv("packet", packet)
                ));
            }
        }
    }
}


